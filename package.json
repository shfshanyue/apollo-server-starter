{
  "name": "apollo-server-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node index.ts",
    "dev": "nodemon index.ts",
    "ncu": "ncu",
    "env:db": "docker-compose -f db.compose.yml up",
    "debug": "nodemon --exec 'node --inspect=0.0.0.0:9930 --require ts-node/register index.ts'",
    "config": "ts-node scripts/pullConfig",
    "schema": "ts-node scripts/createSchema",
    "migrate": "sequelize db:migrate --config ./config/db.js --migrations-path ./db/migrations",
    "migrate:new": "sequelize migration:generate --config ./config/db.js --migrations-path ./db/migrations --name $name",
    "migrate:undo": "sequelize db:migrate:undo --config ./config/db.js --migrations-path ./db/migrations",
    "seed": "sequelize db:seed:all --config ./config/db.js --seeders-path ./db/seeders",
    "seed:new": "sequelize seed:generate --config ./config/db.js --seeders-path ./db/seeders --name $name",
    "seed:undo": "sequelize db:seed:undo --config ./config/db.js --seeders-path ./db/seeders --name $name",
    "log:db": "tail -f logs/db.log  | jq '. | {message, requestId}'",
    "log": "tail -f logs/$LOG.log | jq '. | {message, requestId}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/utils": "^8.6.1",
    "@sentry/node": "^6.17.9",
    "@sideway/address": "^4.1.3",
    "@sideway/formula": "^3.0.0",
    "@sideway/pinpoint": "^2.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/cls-hooked": "^4.3.3",
    "@types/consul": "^0.40.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/hapi__joi": "^17.1.8",
    "@types/ioredis": "^4.28.8",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/lodash": "^4.14.178",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^17.0.18",
    "@types/rimraf": "^3.0.2",
    "@types/sequelize": "^4.28.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-koa": "^3.6.3",
    "apollo-server-plugin-response-cache": "^3.5.1",
    "axios": "^0.26.0",
    "bluebird": "^3.7.2",
    "cls-hooked": "^4.2.2",
    "consul": "^0.40.0",
    "dataloader-sequelize": "^2.3.3",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.28.5",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.13.4",
    "lodash": "^4.17.21",
    "lru-cache": "^7.3.1",
    "moment": "^2.29.1",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.3.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "ts-node": "^10.5.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "typescript": "^4.5.5"
  }
}
